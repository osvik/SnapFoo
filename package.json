{
  "name": "snapfoo",
  "version": "2.1.1",
  "description": "SnapFoo is an animation library that compliments Snap.svg to simplify the SVG animation process.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watch:js",
    "watch:js": "babel -w ./src/js/snapfoo.js --out-file ./dist/js/snapfoo.js --presets=es2015",
    "build": "npm run build:minifyjs",
    "build:minifyjs": "minify ./dist/js/snapfoo.js",
    "test": "mocha"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "minifier": "^0.8.1",
    "yarn-run-all": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/yuschick/snapfoo"
  },
  "keywords": [
    "snapfoo",
    "snapsvg",
    "svg",
    "animation",
    "jquery"
  ],
  "author": "Daniel Yuschick",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "jquery": "^3.2.1",
    "snapsvg": "^0.5.1"
  }
}
